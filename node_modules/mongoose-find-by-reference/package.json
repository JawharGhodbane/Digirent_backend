{
  "name": "mongoose-find-by-reference",
  "author": "CheezOne",
  "homepage": "https://github.com/cheezone/mongoose-find-by-reference",
  "version": "1.0.3",
  "description": "Make Mongoose support finding in reference fields.",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc --declaration --module commonjs",
    "build:tsup": "tsup index.ts --dts --format cjs,esm",
    "release": "tsup index.ts --dts --format cjs,esm && npm publish",
    "lint": "eslint --fix --ext .ts ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cheezone/mongoose-find-by-reference.git"
  },
  "bugs": {
    "url": "https://github.com/cheezone/mongoose-find-by-reference/issues"
  },
  "keywords": [
    "mongoose",
    "reference",
    "ref",
    "objectid",
    "mongodb"
  ],
  "license": "Apache 2.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "mongoose": "6.2.1"
  },
  "tsup": {
    "outDir": "./"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    }
  }
}